.footer-page .footer-top{background-color: #169a5a ; color: #fff; padding: 40px 0 35px; width: 100%;}
.footer-page .footer-top a{color: rgba(255,255,255,0.75);}
.footer-page .footer-top a:hover, .footer-page .footer-top a:focus{color: #fff; text-decoration: none;}
.footer-bottom {padding-bottom: 10px;padding-top: 10px;}
.footer-bottom a{color: #333; margin: 0 5px;}
.fa-facebook-square{color: #3C5A99; font-size: 18px;}
.fa-youtube-play{color: #FF0000;}

@media (min-width:992px) {
    .d-lg-up{
        display: none !important;
    }
}

.user-name {
    color: #ffffff;
    font-weight: bold;
}

ul a {
    color: #14965c;
}

ul a:hover {
    background-color: rgb(236, 233, 233);
    color: #14965c !important;
}

.navbar-nav .nav-item:not(:last-child) {
    border-right: 1px solid rgba(0, 0, 0, 0.125);
 }
 
 @media (max-width: 768px) {
   .navbar-nav .nav-item:not(:last-child) {
     border-right: none;
   }
 }

 .nav-link {
    padding: 0.2rem;
 }