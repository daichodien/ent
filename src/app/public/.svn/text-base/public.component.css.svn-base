.wrapper {
    min-height: 550px;
}
.header-page {
    background-color: #14965c; 
    width: 100%; 
    position: relative; 
    z-index: 1;
    padding-bottom: 20px;
}

.logo {
    margin: 0; 
    display: inline-block; 
    vertical-align: top;
}

.logo a, .logo img {
    display: block;
}

.logo {
    padding-top: 10px; 
    height: 36px;
}

